<?xml version="1.0" encoding="utf-8"?>

<Defs>

  <ThingDef ParentName="ResourceBase">
    <defName>MSSMeme_BowlOne</defName>
    <label>coal bowl</label>
    <description>A bowl made of coal. You could deconstruct it to get a small amount of whatever coal this bowl is made from, or you could scatter it somewhere. It is a nice looking bowl after all. Who knows, it may even have a use.</description>
    <possessionCount>1</possessionCount>
    <graphicData>
      <texPath>Things/Item/Resource/MSSMeme_Bowl</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <color>(60,60,60)</color>
    </graphicData>
		<stackLimit>1</stackLimit>
		<healthAffectsPrice>false</healthAffectsPrice>
		<intricate>true</intricate> <!-- Not returned to player when product made of this is smelted. -->
		<statBases>
			<MaxHitPoints>20</MaxHitPoints>
			<MarketValue>2.3</MarketValue>
			<Mass>0.1</Mass>
			<Beauty>8</Beauty>
			<Flammability>2.0</Flammability>
		</statBases>
		<thingCategories>
			<li>Manufactured</li>
		</thingCategories>
  </ThingDef>

